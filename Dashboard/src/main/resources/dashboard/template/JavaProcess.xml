<?xml version="1.0"?>
<template>
	<width>1024</width>
	<height>600</height>
	<background>
		<objectType>IMAGE</objectType>
		<src>resources/images/user/default_image001.png</src>
	</background>
	<resources>
		<resource>
			<objectName>ENS.TextBoxElementView</objectName>
			<x>312</x>
			<y>20</y>
			<w>400</w>
			<h>30</h>
			<property>
				<border>
					<fill>#333333</fill>
					<stroke>#000000</stroke>
					<strokeWidth>2</strokeWidth>
					<strokeDasharray></strokeDasharray>
				</border>
				<label>
					<fontSize>24</fontSize>
					<textAnchor>middle</textAnchor>
					<fontFamily>Arial</fontFamily>
					<fill>#ffffff</fill>
					<text>Java Process</text>
				</label>
			</property>
		</resource>
		<resource>
			<objectName>ENS.MultipleResourceGraphElementView</objectName>
			<x>50</x>
			<y>65</y>
			<w>400</w>
			<h>220</h>
			<property>
				<resourceId></resourceId>
				<name>CPU Usage(OS):%</name>
				<target>.*/system/cpu/usage/total.*</target>
			</property>
		</resource>
		<resource>
			<objectName>ENS.MultipleResourceGraphElementView</objectName>
			<x>500</x>
			<y>65</y>
			<w>400</w>
			<h>220</h>
			<property>
				<resourceId></resourceId>
				<name>CPU Usage(Process):%</name>
				<target>.*/process/cpu/usage/total.*</target>
			</property>
		</resource>
		<resource>
			<objectName>ENS.MultipleResourceGraphElementView</objectName>
			<x>50</x>
			<y>320</y>
			<w>400</w>
			<h>220</h>
			<property>
				<resourceId></resourceId>
				<name>Memory Usage(Heap)</name>
				<target>.*/process/heap/used.*</target>
			</property>
		</resource>
		<resource>
			<objectName>ENS.MultipleResourceGraphElementView</objectName>
			<x>500</x>
			<y>320</y>
			<w>400</w>
			<h>220</h>
			<property>
				<resourceId></resourceId>
				<name>GC Time</name>
				<target>.*/process/gc/time/total.*</target>
			</property>
		</resource>
	</resources>
</template>