<?xml version="1.0"?>
<template>
	<width>1024</width>
	<height>600</height>
	<background>
		<objectType>POLYGON</objectType>
		<fill>#000000</fill>
	</background>
	<resources>
		<resource>
			<objectName>ENS.TextBoxElementView</objectName>
			<x>312</x>
			<y>20</y>
			<w>400</w>
			<h>30</h>
			<property>
				<border>
					<fill>#444444</fill>
					<stroke>#000000</stroke>
					<strokeWidth>2</strokeWidth>
					<strokeDasharray></strokeDasharray>
				</border>
				<label>
					<fontSize>24</fontSize>
					<textAnchor>middle</textAnchor>
					<fontFamily>Arial</fontFamily>
					<fill>#ffffff</fill>
					<text>Java Application</text>
				</label>
			</property>
		</resource>
		<resource>
			<objectName>ENS.MultipleResourceGraphElementView</objectName>
			<x>50</x>
			<y>65</y>
			<w>850</w>
			<h>220</h>
			<property>
				<resourceId></resourceId>
				<name>CPU Usage(OS):%</name>
				<target>/${ClusterName}/.*/system/cpu/usage/total.*</target>
			</property>
		</resource>
		<resource>
			<objectName>ENS.MultipleResourceGraphElementView</objectName>
			<x>50</x>
			<y>320</y>
			<w>400</w>
			<h>220</h>
			<property>
				<resourceId></resourceId>
				<name>CPU Usage(Process):%</name>
				<target>/${ClusterName}/.*/process/cpu/usage/total.*</target>
			</property>
		</resource>
		<resource>
			<objectName>ENS.MultipleResourceGraphElementView</objectName>
			<x>500</x>
			<y>320</y>
			<w>400</w>
			<h>220</h>
			<property>
				<resourceId></resourceId>
				<name>Exception</name>
				<target>/${ClusterName}/.*/event/.*</target>
			</property>
		</resource>
		<resource>
			<objectName>ENS.SignalElementView</objectName>
			<x>0</x>
			<y>0</y>
			<w>50</w>
			<h>50</h>
			<property>
				<signal>
					<resourceId>/ClusterA/127.0.0.1/agent_000/system/cpu/usage/user:%/TestReSignal4</resourceId>
					<objectType>SINGLE</objectType>
					<name>SampleSignal</name>
					<level>3</level>
					<threshold>1,5</threshold>
					<target>/ClusterA/127.0.0.1/agent_000/system/cpu/usage/user:%/TestSignal4</target>
					<period>10</period>
				</signal>
				<label>
					<fontSize>12</fontSize>
					<textAnchor>middle</textAnchor>
					<fontFamily>Arial</fontFamily>
					<fill>#ffffff</fill>
				</label>
			</property>
		</resource>
	</resources>
</template>